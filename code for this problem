#include <stdio.h>

int main(void) {
	int t,n,x,y,z,count,i,j,a,b,c;
	scanf("%d",&t);
	while(t--)
	{
	  scanf("%d %d",&x,&y);
	  if(x+y<3)
      printf("1\n");
    else if(x+y>=3&&x+y<=10)
       printf("2\n");
    else if(x+y>=11&&x+y<=60)
       printf("3\n");
    else
       printf("4\n");
	}
	return 0;
}

/*Output Format
For each testcase, output in a single line, answer 1 for bullet, 2 for blitz, 3 for rapid, and 4 for classical format.*/
